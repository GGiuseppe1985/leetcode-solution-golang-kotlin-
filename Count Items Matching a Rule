func countMatches(items [][]string, ruleKey string, ruleValue string) int {
    count := 0
    for _, item := range items {
        if ruleKey == "type"  && ruleValue == item[0]{
            count ++
        } else if ruleKey == "color" && ruleValue == item[1]{
            count++
        } else if ruleKey == "name" && ruleValue == item[2]{
            count++
        }
    }
    return count

}
